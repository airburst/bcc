{
  "name": "bcc",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "check-types": "tsc --noEmit --incremental --emitDeclarationOnly false --pretty",
    "postinstall": "prisma generate",
    "connect": "pscale connect bcc-rides main --port 3309",
